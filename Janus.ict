          }

          else if (ct.room.name == "B2") {
              ct.rooms.switch("B4");
          }
        arguments: {}
        lib: core
      - eventKey: OnCreate
        code: >-
          this.tint = 0xb190ee;

          this.alpha = 0.9;


          if (ct.room.name == "door01") {
              const lineA = "(-1) study and graduate as fast as possible";
