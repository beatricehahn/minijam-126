        code: |
          ct.sound.spawn('doorSelect');
          // if the current room is door one
          if (ct.room.name == "door01") {
              ct.rooms.switch("A1");
          }

          else if (ct.room.name == "A1") {
              ct.rooms.switch("A2");
          }

          else if (ct.room.name == "A2") {
              ct.rooms.switch("A4");
          }

          else if (ct.room.name == "A3") {
              ct.rooms.switch("A6");
          }

          else if (ct.room.name == "B1") {
